{
  "name": "horizon-react-extend",
  "version": "0.3.0",
  "description": "React bindings for horizon, fork from package horizon-react",
  "main": "./lib/index.js",
  "scripts": {
    "build:lib": "NODE_ENV=development babel src --out-dir lib",
    "build:umd": "cross-env NODE_ENV=development webpack src/index.js dist/horizon-react.js",
    "build:umd:min": "cross-env NODE_ENV=production webpack src/index.js dist/horizon-react.min.js",
    "build": "npm run build:lib && npm run build:umd && npm run build:umd:min && node ./prepublish",
    "prepublish": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/linxiaowu66/horizon-react.git"
  },
  "keywords": [
    "react",
    "horizon"
  ],
  "author": "Patrick Neschkudla <neschkudla@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/linxiaowu66/horizon-react/issues"
  },
  "homepage": "https://github.com/linxiaowu/horizon-react#readme",
  "devDependencies": {
    "babel-cli": "6.7.5",
    "babel-core": "6.7.5",
    "babel-eslint": "6.0.0",
    "babel-loader": "6.2.4",
    "babel-plugin-react-transform": "2.0.2",
    "babel-plugin-syntax-jsx": "6.3.13",
    "babel-plugin-transform-class-properties": "6.4.0",
    "babel-plugin-transform-runtime": "6.7.5",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-stage-0": "6.5.0",
    "babel-register": "^6.3.13",
    "cross-env": "^1.0.7",
    "es3ify": "^0.2.0",
    "eslint": "^2.8.0",
    "eslint-config-airbnb": "^8.0.0",
    "eslint-plugin-import": "^1.5.0",
    "eslint-plugin-jsx-a11y": "^1.0.2",
    "eslint-plugin-react": "^5.0.1",
    "glob": "^6.0.4"
  },
  "dependencies": {
    "@horizon/client": "^1.0.1",
    "is-plain-object": "^2.0.1",
    "lodash.isequal": "^4.1.4",
    "react-dom": "^15.0.1",
    "react-redux": "^4.4.5",
    "exports-loader": "^0.6.3",
    "imports-loader": "^0.6.5",
    "redux": "^3.5.2"
  },
